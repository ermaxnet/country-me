{
  "name": "country_me",
  "version": "1.0.0",
  "description": "Приложение для просмотра актуальной информации по стране посетителя, или по запросу конкретной страны. Так же приложение открывает API для получения информации о случайной стране",
  "main": "application.js",
  "scripts": {
    "build": "webpack-cli --mode development",
    "start": "APPLICATION_ENVIRONMENT=development node server.js",
    "build-production": "webpack-cli --mode production",
    "start-production": "APPLICATION_ENVIRONMENT=production PORT=80 node server.js",
    "deploy": "run-s build-production start-production"
  },
  "keywords": [
    "restcountries",
    "node.js",
    "express",
    "react"
  ],
  "author": "Max Eremin",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "normalize.css": "^8.0.0",
    "pug": "^2.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-pug-plugin": "^0.3.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.23.1"
  }
}
